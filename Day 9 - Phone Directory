#include <bits/stdc++.h>
using namespace std;

class Solution{
public:
    vector<vector<string>> displayContacts(int n, string contact[], string s)
    {
        map<string,int>mp;
        for(int i=0;i<n;i++) {
            mp[contact[i]]++;
        }
        
        vector<vector<string>>ans;
        
        for(int i=0;i<s.size();i++){
            vector<string> v;
            
            for(auto it:mp) {
                if(it.first.substr(0, i+1)==s.substr(0, i+1))
                    v.push_back(it.first);
                    
                    
            }
            
            if(v.size()==0)
                v.push_back("0");
                
            ans.push_back(v);    
        }
        return ans;
    }
};

int main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string contact[n], s;
        for(int i = 0;i < n;i++)
            cin>>contact[i];
        cin>>s;
        
        Solution ob;
        vector<vector<string>> ans = ob.displayContacts(n, contact, s);
        for(int i = 0;i < s.size();i++){
            for(auto u: ans[i])
                cout<<u<<" ";
            cout<<"\n";
        }
    }
    return 0;
}
